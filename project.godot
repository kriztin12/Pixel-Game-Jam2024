; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Pixel Game Jam"
run/main_scene="res://scenes/menu.tscn"
config/features=PackedStringArray("4.2", "GL Compatibility")
config/icon="res://sailing_icon.svg"

[autoload]

Global="*res://scripts/global.gd"
SceneManager="*res://scripts/scene_manager.gd"
Dialogic="*res://addons/dialogic/Core/DialogicGameHandler.gd"

[dialogic]

directories/dch_directory={
"jellychan": "res://characters/jellychan.dch",
"krillito": "res://characters/krillito.dch",
"mystery": "res://characters/mystery.dch",
"player": "res://characters/player.dch",
"victor": "res://characters/victor.dch"
}
directories/dtl_directory={
"after_dance": "res://timelines/after_dance.dtl",
"after_gaming": "res://timelines/after_gaming.dtl",
"dodgeball_scene": "res://timelines/dodgeball_scene.dtl",
"end_scene": "res://timelines/end_scene.dtl",
"enter_school": "res://timelines/enter_school.dtl",
"gaming_jelly": "res://timelines/gaming_jelly.dtl",
"prologue": "res://timelines/prologue.dtl",
"start_scene": "res://timelines/start_scene.dtl",
"test": "res://test.dtl"
}
glossary/default_case_sensitive=true
layout/style_list=["res://styles/styles.tres"]
layout/default_style="res://styles/styles.tres"
variables={
"playerName": "",
"playerScore": 0
}
text/letter_speed=0.01
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/autoadvance_per_character_delay=0.1
text/autoadvance_ignored_characters_enabled=true
animations/join_default_length=0.5
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/join_default_wait=true
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}

[display]

window/size/viewport_width=128
window/size/viewport_height=108
window/size/resizable=false
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/scale_mode="integer"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg")

